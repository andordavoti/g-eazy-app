{
  "expo": {
    "name": "G-Eazy",
    "slug": "g-eazy-app",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.ovitas.geazy",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 20,
      "package": "com.andordavoti.geazyalbums"
    }
  }
}
